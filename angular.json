{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "edc-popover-ng-app": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "edc",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": true,
          "style": "less"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/edc-popover-ng-app",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.less"
            ],
            "scripts": [],
            "aot": false,
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "edc-popover-ng-app:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "edc-popover-ng-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "edc-popover-ng-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.less"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        }
      }
    },
    "edc-popover-ng-app-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "edc-popover-ng-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "edc-popover-ng-app:serve:production"
            }
          }
        }
      }
    },
    "edc-popover-ng": {
      "root": "projects/edc-popover-ng",
      "sourceRoot": "projects/edc-popover-ng/src",
      "projectType": "library",
      "prefix": "edc",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/edc-popover-ng/tsconfig.lib.json",
            "project": "projects/edc-popover-ng/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/edc-popover-ng/ng-package.prod.json"
            ,              "tsConfig": "projects/edc-popover-ng/tsconfig.lib.prod.json"
}
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/edc-popover-ng/src/test.ts",
            "tsConfig": "projects/edc-popover-ng/tsconfig.spec.json",
            "karmaConfig": "projects/edc-popover-ng/karma.conf.js"
          }
        }
      }
    }
  }
}
